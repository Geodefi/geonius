{
  "directory": "geonius",
  "logger": {
    "file": true,
    "stream": true,
    "directory": "logs",
    "level": "DEBUG",
    "when": "midnight",
    "interval": 1,
    "backup": 30
  },
  "database": {
    "directory": "db"
  },
  "chains": {
    "holesky": {
      "start": 1550551,
      "identifier": "latest",
      "period": "1",
      "interval": "12",
      "range": "3000",
      "slots_per_epoch": 32
    }
  },
  "ethdo": {
    "wallet": "geonius",
    "account": "validators"
  },
  "email": {
    "smtp_server": "smtp.gmail.com",
    "smtp_port": 587,
    "admin_email": "bandicoot@geode.fi"
  }
}
