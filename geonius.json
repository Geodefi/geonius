{
  "directory": ".geonius",
  "strategy": {
    "min_proposal_queue": 0,
    "max_proposal_delay": 0
  },
  "network": {
    "refresh_rate": 60,
    "max_attempt": 20,
    "attempt_rate": 0.1
  },
  "gas": {
    "max_priority": 3,
    "max_fee": 100,
    "api": "https://gas.api.infura.io/v3/<GAS_API_KEY>/networks/1/suggestedGasFees",
    "parser": {
      "base": "low.suggestedMaxFeePerGas",
      "priority": "low.suggestedMaxPriorityFeePerGas"
    }
  },
  "logger": {
    "file": true,
    "stream": true,
    "directory": "logs",
    "level": "INFO",
    "when": "midnight",
    "interval": 1,
    "backup": 30
  },
  "database": {
    "directory": "db"
  },
  "chains": {
    "holesky": {
      "start": 1550551,
      "identifier": "latest",
      "period": "1",
      "interval": "12",
      "range": "3000",
      "slots_per_epoch": 32
    }
  },
  "ethdo": {
    "wallet": "geonius",
    "account": "validators"
  },
  "email": {
    "smtp_server": "smtp.gmail.com",
    "smtp_port": 587,
    "admin_email": "bandicoot@geode.fi",
    "notify_geode": true
  }
}
